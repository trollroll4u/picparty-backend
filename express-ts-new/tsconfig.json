{
    "compilerOptions": {
        "target": "es2016",
        "module": "CommonJS",
        "outDir": "./build",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*"]
}